{
    "name": "mdigi/laraview",
    "description": "Laravel View Template",
    "type": "project",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Abdul Aziz Nurfarhana",
            "email": "farhan@mdigi.tech"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mdigi\\LaraView\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mdigi\\LaraView\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter"
    },
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mdigi\\LaraView\\LaraViewServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
